{
  "name": "DatPart",
  "productName": "DatPart",
  "description": "Load Dat's in your web browser.",
  "homepage": "https://github.com/HughIsaacs2/DatPart",
  "repository": {
    "type": "git",
    "url": "https://github.com/HughIsaacs2/DatPart.git"
  },
  "version": "0.0.7",
  "author": "Hugh Isaacs II <hughisaacs@acrylicstyle.com>",
  "license": "MIT",
  "main": "launch.js",
  "dependencies": {},
  "build": {
    "files": [
      "node_modules/",
      "favicon.ico",
      "logo.png",
      "logo_16.png",
      "logo_128.png",
      "logo.svg",
      "style.css",
      "launch.js",
      "display_script.js",
      "server_script.js",
      "index.html",
      "server_app.html",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "ia32"
          ]
        }
      ],
      "icon": "favicon.ico"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "devDependencies": {
    "dat-node": "^3.5.11",
    "electron": "3.0.8",
    "electron-rebuild": "^1.7.3"
  }
}

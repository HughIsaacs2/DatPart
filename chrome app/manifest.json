       "tcpServer": {
         "listen": ["*"]
       }
    },
  "manifest_version": 2,
  "minimum_chrome_version": "72",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["launch.js"]
    }
  }
}